<?xml version="1.0" encoding="utf-8"?><skconfig name="AntiPkgLoss 0.0.1 Configuration">	
	<!--Architecture to build-->	
	<build actived="amd64">		
		<!--Supported Architecture-->		
		<arch name="x86">			
			<!--Build options,projects's private options can override them-->			
			<AS type="textbox">gcc</AS>			
			<ASFLAGS type="textbox">-m32 -c</ASFLAGS>
			<ASRULE type="textbox">$(AS) $(ASFLAGS) -o $@ $&lt;</ASRULE>
			<CC type="textbox">gcc</CC>	
			<CFLAGS type="textbox">-c</CFLAGS>
			<CCRULE type="textbox">$(CC) $(CFLAGS) -o $@ $&lt;</CCRULE>
			<LD type="textbox">gcc</LD>
			<LDFLAGS type="textbox"></LDFLAGS>
			<LDRULE type="textbox">$(LD) $(LDFLAGS) -o $@ $^</LDRULE>
			<DEP type="textbox">gcc</DEP>
			<DEPRULE type="textbox">$(DEP) -MM -o $@ $&lt;;if [ $$? == 0 ];then echo $(OBJDIR)/$(ARCH)/`cat $@` &gt; $@; fi</DEPRULE>
			<AFTER type="textbox"></AFTER>		
		</arch>
		<arch name="amd64">			
			<!--Build options,projects's private options can override them-->			
			<AS type="textbox">gcc</AS>			
			<ASFLAGS type="textbox">-c</ASFLAGS>
			<ASRULE type="textbox">$(AS) $(ASFLAGS) -o $@ $&lt;</ASRULE>
			<CC type="textbox">gcc</CC>	
			<CFLAGS type="textbox">-c</CFLAGS>
			<CCRULE type="textbox">$(CC) $(CFLAGS) -o $@ $&lt;</CCRULE>
			<LD type="textbox">gcc</LD>
			<LDFLAGS type="textbox"></LDFLAGS>
			<LDRULE type="textbox"></LDRULE>
			<DEP type="textbox">gcc</DEP>
			<DEPRULE type="textbox">$(DEP) -MM -o $@ $&lt;;if [ $$? == 0 ];then echo $(OBJDIR)/$(ARCH)/`cat $@` &gt; $@; fi</DEPRULE>
			<AFTER type="textbox"></AFTER>
		</arch>	
	</build>	
	<targets>		
		<target build="true" name="client" src="src/client"/>
		<target build="true" name="server" src="src/server"/>
	</targets>	
</skconfig>